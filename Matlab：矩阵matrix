A=[5,10,15;0,15,15;35,45,25]
B=[8 3 9;2 8 1;3 9 1]
A.*A
flipud(A.*A)%注意A点乘A 和A乘A是不同的
A.*B
inv(A)%取逆矩阵
det(A*B)

A=[1,2,-1;
    3,1,0;
    -1,0,-2]
inv(A)*det(A)%求伴随矩阵
inv(A)%求逆矩阵
B=[0,3,3;
    1,1,0;
    -1,2,3]
   C=inv(A)
   C*B%矩阵相乘
format rat%所有结果用分数表示，不用小数


A=[0,1,-3,4,5;
    1,0,-2,3,4;
   3,2,0,-5,2;
   4,3,-5,0,5]
 
format rat
rref(A)%化为行最简形矩阵
